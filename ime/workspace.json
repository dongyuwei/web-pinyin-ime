{
  "version": 2,
  "projects": {
    "ime": {
      "root": "apps/ime",
      "sourceRoot": "apps/ime/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired build",
            "cwd": "apps/ime",
            "outputPath": "dist/apps/ime"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired start",
            "cwd": "apps/ime"
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/eslint",
            "cwd": "apps/ime"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired test --watchAll=false",
            "cwd": "apps/ime"
          }
        }
      }
    },
    "ime-e2e": {
      "root": "apps/ime-e2e",
      "sourceRoot": "apps/ime-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx e2e-serve ime-e2e",
              "nx e2e-run ime-e2e"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/ime-e2e/**/*.{js,ts}"
            ]
          }
        },
        "e2e-run": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ime-e2e/cypress.json",
            "tsConfig": "apps/ime-e2e/tsconfig.e2e.json",
            "baseUrl": "http://localhost:3000"
          }
        },
        "e2e-serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx serve ime"
            ],
            "readyWhen": "can now view"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "ime"
}